<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dromara.witdock.mapper.DatasetDocMapper">

    <select id="countCharNumByDatasetId" resultType="java.lang.Integer">
        select sum(char_num)
        from dataset_doc
        where dataset_id = #{datasetId}
    </select>

</mapper>
